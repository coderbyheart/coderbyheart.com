{
  "name": "@coderbyheart/coderbyheart.com",
  "version": "0.0.0-development",
  "description": "Source code for coderbyheart.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "postinstall": "check-node-version --package",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "build": "gatsby build --prefix-paths && node critical-css.js",
    "start": "gatsby develop"
  },
  "keywords": [
    "Markus Tacker",
    "coderbyheart",
    "homepage",
    "profile",
    "GatsbyJS",
    "React",
    "TypeScript",
    "Netlify",
    "GitHub Actions"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "UNLICENSED",
  "homepage": "https://github.com/coderbyheart/coderbyheart.com",
  "bugs": {
    "url": "https://github.com/coderbyheart/coderbyheart.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderbyheart/coderbyheart.com.git"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "hast-to-hyperscript": "9.0.0",
    "polished": "4.0.2",
    "styled-components": "5.2.0"
  },
  "snyk": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@bifravst/code-style": "8.0.134",
    "@bifravst/eslint-config-typescript": "5.0.66",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/classnames": "^2.2.10",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.4",
    "babel-plugin-styled-components": "1.11.1",
    "chalk": "^4.1.0",
    "check-node-version": "4.0.3",
    "contentful": "^7.14.7",
    "contentful-management": "^6.1.2",
    "eslint-plugin-react": "7.21.4",
    "gatsby": "2.24.77",
    "gatsby-cli": "2.12.107",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-styled-components": "3.3.14",
    "gatsby-plugin-typescript": "2.4.21",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-filesystem": "2.3.34",
    "gatsby-transformer-remark": "2.8.39",
    "husky": "4.3.0",
    "jsdom": "16.4.0",
    "lint-staged": "10.4.0",
    "minimalcss": "0.11.0",
    "node-static": "0.7.11",
    "prettier": "2.1.2",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
    "rehype-react": "6.1.0",
    "snyk": "1.413.3",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
