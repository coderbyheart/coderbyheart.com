{
  "name": "@coderbyheart/coderbyheart.com",
  "version": "0.0.0-development",
  "description": "Source code for coderbyheart.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "postinstall": "check-node-version --package",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "build": "gatsby build --prefix-paths && node critical-css.js",
    "start": "gatsby develop"
  },
  "keywords": [
    "Markus Tacker",
    "coderbyheart",
    "homepage",
    "profile",
    "GatsbyJS",
    "React",
    "TypeScript",
    "Netlify",
    "GitHub Actions"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "UNLICENSED",
  "homepage": "https://coderbyheart.com",
  "bugs": {
    "url": "https://github.com/coderbyheart/coderbyheart.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderbyheart/coderbyheart.com.git"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "date-fns": "2.25.0",
    "gatsby-plugin-disqus": "1.2.3",
    "gatsby-remark-prismjs": "6.0.0",
    "hast-to-hyperscript": "10.0.1",
    "polished": "4.1.3",
    "prismjs": "1.25.0",
    "react": "17.0.2",
    "react-intersection-observer": "8.32.2",
    "remark-directive": "1.0.1",
    "styled-components": "5.3.3"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "devDependencies": {
    "@commitlint/config-angular": "13.2.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.2.8",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "8.1.12",
    "@types/classnames": "2.3.1",
    "@types/react-dom": "17.0.10",
    "@types/react-helmet": "6.1.4",
    "@types/styled-components": "5.1.15",
    "babel-plugin-styled-components": "1.13.3",
    "bottleneck": "2.19.5",
    "chalk": "4.1.2",
    "check-node-version": "4.1.0",
    "contentful": "9.1.4",
    "contentful-management": "7.45.0",
    "eslint-plugin-react": "7.26.1",
    "gatsby": "4.0.2",
    "gatsby-cli": "4.0.0",
    "gatsby-plugin-react-helmet": "5.0.0",
    "gatsby-plugin-react-helmet-canonical-urls": "1.4.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-styled-components": "5.0.0",
    "gatsby-plugin-typescript": "4.0.0",
    "gatsby-remark-autolink-headers": "5.0.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-filesystem": "4.0.0",
    "gatsby-transformer-remark": "5.0.0",
    "jsdom": "18.0.0",
    "minimalcss": "0.11.2",
    "node-fetch": "3.0.0",
    "node-static": "0.7.11",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "rehype-react": "7.0.3",
    "ts-node": "10.4.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
