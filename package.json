{
  "name": "coderbyheart.com",
  "description": "Source code for coderbyheart.com",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderbyheart/coderbyheart.com.git"
  },
  "keywords": [
    "homepage",
    "profile"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "UNLICENSED",
  "homepage": "https://github.com/coderbyheart/coderbyheart.com",
  "dependencies": {
    "bootstrap": "git://github.com/twbs/bootstrap.git#v4-dev",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "font-awesome": "^4.6.3",
    "jquery": "^3.0.0",
    "material-design-icons": "^2.2.3",
    "node-sass": "^3.7.0",
    "tether": "^1.3.2",
    "uglifycss": "0.0.21",
    "uglifyjs": "^2.4.10"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "ghooks": "^1.2.3",
    "istanbul": "^0.4.3",
    "standard": "^7.1.2",
    "validate-commit-msg": "^2.7.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "only": [
            "/assets/js/*.js"
          ]
        }
      ],
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "tether": "./node_modules/tether/dist/js/tether.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "tether",
        "jquery"
      ]
    }
  }
}
