{
  "name": "@coderbyheart/coderbyheart.com",
  "version": "0.0.0-development",
  "description": "Source code for coderbyheart.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "postinstall": "check-node-version --package",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "build": "NODE_OPTIONS=--no-experimental-fetch gatsby build --prefix-paths && node critical-css.js",
    "start": "NODE_OPTIONS=--no-experimental-fetch gatsby develop"
  },
  "keywords": [
    "Markus Tacker",
    "coderbyheart",
    "homepage",
    "profile",
    "GatsbyJS",
    "React",
    "TypeScript",
    "Netlify",
    "GitHub Actions"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "UNLICENSED",
  "homepage": "https://coderbyheart.com",
  "bugs": {
    "url": "https://github.com/coderbyheart/coderbyheart.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderbyheart/coderbyheart.com.git"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "gatsby-plugin-disqus": "1.2.4",
    "gatsby-remark-prismjs": "6.18.0",
    "hast-to-hyperscript": "10.0.1",
    "polished": "4.2.2",
    "prismjs": "1.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.3.5",
    "styled-components": "5.3.5"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "devDependencies": {
    "@commitlint/config-angular": "17.0.3",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.98",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "10.0.29",
    "@types/classnames": "2.3.1",
    "@types/react-dom": "18.0.6",
    "@types/react-helmet": "6.1.5",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "babel-plugin-styled-components": "2.0.7",
    "bottleneck": "2.19.5",
    "chalk": "4.1.2",
    "check-node-version": "4.2.1",
    "contentful": "9.1.32",
    "contentful-management": "10.7.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-unicorn": "43.0.0",
    "gatsby": "4.18.0",
    "gatsby-cli": "4.18.0",
    "gatsby-plugin-react-helmet": "5.18.0",
    "gatsby-plugin-react-helmet-canonical-urls": "1.4.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-styled-components": "5.18.0",
    "gatsby-plugin-typescript": "4.18.0",
    "gatsby-remark-autolink-headers": "5.18.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-filesystem": "4.18.0",
    "gatsby-transformer-remark": "5.18.0",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "minimalcss": "0.11.3",
    "node-fetch": "3.2.6",
    "node-static": "0.7.11",
    "react-helmet": "6.1.0",
    "rehype-react": "7.1.1",
    "ts-node": "10.8.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
