{
  "name": "@coderbyheart/coderbyheart.com",
  "version": "0.0.0-development",
  "description": "Source code for coderbyheart.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "husky install",
    "postinstall": "check-node-version --package",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "build": "gatsby build --prefix-paths && node critical-css.js",
    "start": "gatsby develop"
  },
  "keywords": [
    "Markus Tacker",
    "coderbyheart",
    "homepage",
    "profile",
    "GatsbyJS",
    "React",
    "TypeScript",
    "Netlify",
    "GitHub Actions"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "UNLICENSED",
  "homepage": "https://coderbyheart.com",
  "bugs": {
    "url": "https://github.com/coderbyheart/coderbyheart.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderbyheart/coderbyheart.com.git"
  },
  "dependencies": {
    "classnames": "2.3.0",
    "date-fns": "2.19.0",
    "gatsby-plugin-disqus": "1.2.2",
    "gatsby-remark-attr": "0.1.0",
    "gatsby-remark-prismjs": "4.2.0",
    "hast-to-hyperscript": "9.0.1",
    "polished": "4.1.1",
    "prismjs": "1.23.0",
    "react": "17.0.2",
    "react-intersection-observer": "8.31.0",
    "styled-components": "5.2.3"
  },
  "snyk": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-angular": "12.0.1",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "^9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "7.0.11",
    "@types/classnames": "2.2.11",
    "@types/react-dom": "17.0.2",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.8",
    "babel-plugin-styled-components": "1.12.0",
    "bottleneck": "2.19.5",
    "chalk": "4.1.0",
    "check-node-version": "4.1.0",
    "contentful": "8.2.0",
    "contentful-management": "7.9.0",
    "eslint-plugin-react": "7.22.0",
    "gatsby": "3.0.4",
    "gatsby-cli": "3.0.0",
    "gatsby-plugin-react-helmet": "4.0.0",
    "gatsby-plugin-react-helmet-canonical-urls": "1.4.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-styled-components": "4.0.0",
    "gatsby-plugin-typescript": "3.0.0",
    "gatsby-remark-autolink-headers": "3.0.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-filesystem": "3.0.0",
    "gatsby-transformer-remark": "3.0.0",
    "husky": "5.1.3",
    "jsdom": "16.5.1",
    "lint-staged": "10.5.4",
    "minimalcss": "0.11.2",
    "node-fetch": "2.6.1",
    "node-static": "0.7.11",
    "prettier": "2.2.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "rehype-react": "6.2.0",
    "snyk": "1.485.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
